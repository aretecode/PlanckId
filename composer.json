{
    "name": "aretecode/planck-id",
    "type": "library",
    "version": "0.11.2",
    "description": "Minifies (almost) all HTML ids + classes, & CSS + JS selectors",
    "keywords": ["minify", "shrink", "optimize", "selector", "css", "js", "html", "planck", "planckid", "id", "planck-id"],
    "homepage": "https://github.com/aretecode/planck-id",
    "license": "MIT",
    "authors": [
        {
            "name": "AreteCode"
        }
    ],    
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "phpflo/phpflo": "dev-master",
        "symfony/console": "~2.0",
        "league/event": "2.1.*",
        "illuminate/support": "dev-master",
        "symfony/var-dumper": "3.0.*",
        "league/flysystem": "1.0.*",
        "league/flysystem-memory": "1.0.*"
    },
    "require-dev": {       
        "behat/behat": "~3.0",
        "phpunit/phpunit": "5.1.*"
    },       
    "config": {
        "bin-dir": "bin/"
    },    
    "extra": {
        "branch-alias": {
            "dev-master": "0.11.2-dev"
        }
    },
    "autoload": {
        "psr-4": {"PlanckId\\": ["src/", "tests/"]}, 
        "files": [
            "src/Utilities/functions.php",
            "src/App/setupNodes.php"
        ]
    }
}